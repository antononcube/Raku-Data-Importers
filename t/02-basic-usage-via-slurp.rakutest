use v6.d;

# use lib <. lib>;

use Data::Importers;
use Test;

plan *;

## 1
my $file1 = $*CWD ~ '/resources/short.txt';
is
        slurp($file1, format => 'txt'),
        data-import($file1),
        'txt file equivalence';

## 2
my $file2 = $*CWD ~ '/resources/simple.json';
is
        slurp($file2, format => 'json'),
        data-import($file2),
        'json file equivalence';


done-testing;
